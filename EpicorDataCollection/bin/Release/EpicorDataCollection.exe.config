    <add key="getProyects" value="SELECT idProyecto,DATE_FORMAT(FentregaEstacas,'%Y-%m-%d %H:%i') AS Entrega_Estacas,DATE_FORMAT(FentregaTubos,'%Y-%m-%d') AS Entrega_Arcos,DATE_FORMAT(FentregaAccesorios,'%Y-%m-%d') AS Entrega_Accesorios,DATE_FORMAT(FentregaPlastico,'%Y-%m-%d') AS Entrega_Plastico,DATE_FORMAT(FentregaPerforador,'%Y-%m-%d') AS Entrega_Perforador,DATE_FORMAT(FentregaDobladora,'%Y-%m-%d') AS Entrega_Dobladora FROM tb_seguimiento WHERE status not in ('Proyecto Cerrado') Limit 10;"/>
    <add key="updProjects" value="UPDATE tb_ud SET sincronizar = 1 WHERE idProyecto like '%"/>
    <add key="updErrorProjects" value="UPDATE tb_ud SET sincronizar = 3 WHERE idProyecto like '%"/>
    <!--Queries SQL Epicor-->
    <add key="getEpicorProjects" value="SELECT Character05 AS Proyecto,Key1,Key2,Key3,Key4,Key5,Shortchar03 FROM Ice.UD39 WHERE Company = 'TT' AND Character05 like '%"/>
  
